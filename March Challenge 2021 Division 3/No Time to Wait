#define For(strt,n)     for(int i=strt; i<n; i++)


int main()
{

   int n,h,x;
   cin>>n>>h>>x;
   int arr[n];
   For(0,n)cin>>arr[i];
   int flag=0;
   for(int i=0; i<n; i++){
    int res=arr[i]+x;
    if(res==h){
        flag=1;
        break;
    }
   }
    if(flag==1)cout<< "YES"<<endl;
